generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model employees {
  id     Int     @id @default(autoincrement())
  name   String? @db.VarChar(255)
  role   String? @db.VarChar(255)
  salary Int?
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model guild {
  treasury   Int?
  guild_id   Int     @id @default(autoincrement())
  guild_name String? @db.Text
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model member {
  member_id  Int     @id @default(autoincrement())
  name       String? @db.VarChar(255)
  class      String? @db.VarChar(255)
  rank       String? @db.VarChar(255)
  mission_id Int?
  quests     quests? @relation(fields: [mission_id], references: [mission_id], onDelete: NoAction, onUpdate: SetDefault, map: "member_ibfk_1")

  @@index([mission_id], map: "mission_id")
}

model quests {
  mission_id      Int       @id @default(autoincrement())
  title           String?   @db.Text
  details         String?   @db.Text
  difficulty      String?   @db.VarChar(2)
  reward          Int?
  start_Date      DateTime? @db.Date
  completion_Date DateTime? @db.Date
  member          member[]
}
